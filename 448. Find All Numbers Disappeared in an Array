class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        vector<int>v;
        map<int,int>mp;
        for(int i=0;i<nums.size();i++)
        {
            mp[nums[i]]=1;
        }
        for(int i=1;i<=nums.size();i++)
        {
            if(mp[i]==0)
            {
                v.push_back(i);
            }
        }
        return v;
        
    }
};
